{
  "private": true,
  "scripts": {
    "compile": "npm run bundle && npm run babelify && npm run browserify && rm -rf es2015",
    "bundle": "mkdir -p dist; echo '// jscs:disable' > dist/bundle.js",
    "babelify": "babel ./js --out-dir ./es2015 --presets=es2015",
    "browserify": "browserify ./es2015/**.js | uglifyjs -mc warnings=false >> dist/bundle.js",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "hyperhtml": "^0.7.2",
    "todomvc-app-css": "^2.0.0",
    "todomvc-common": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "browserify": "^14.1.0",
    "uglify-js": "^2.8.11"
  }
}
